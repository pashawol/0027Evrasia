mixin sOrganizers(data)
	// start sOrganizers
	+b.SECTION.sOrganizers.section#sOrganizers&attributes(attributes)
		.container
			+b.section-title(class="wow  animate__fadeIn  " )
				h2= data.title
			.row 
				.col-auto(class="wow  animate__fadeInUp" )
					a(href="https://myrosmol.ru/" target="_blank"): img.res-i(src=`img/svg/${data.image1}`  loading="lazy", alt="")
				.col-auto(class="wow  animate__fadeInUp" )
					a(href="https://orenburg-gov.ru/" target="_blank"): img.res-i(src=`img/svg/${data.image2}`  loading="lazy", alt="")
				.col-auto(class="wow  animate__fadeInUp" )
					a(href=" http://www.rscenter.ru/" target="_blank"): img.res-i(src=`img/svg/${data.image3}`  loading="lazy", alt="")
	// end sOrganizers